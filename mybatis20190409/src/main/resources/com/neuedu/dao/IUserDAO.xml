<?xml version="1.0" encoding="UTF-8" ?>
<!--使用xml实现接口的功能
    约束：mapper
-->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
   根：mapper
      属性：namespace：映射的dao的全限定名：com.neuedu.dao.IUserDAO
-->
<mapper namespace="com.neuedu.dao.IUserDAO">
    <!--查询所有

      使用标签：select
               属性：id：设置时dao中方法名
                     resultType:设置返回的类型：元素类型
    -->
      <select id="selectAll" resultType="com.neuedu.entity.User">
          select uid,uname,upwd,telephone,address,ulevel
          from t_user
      </select>


</mapper>